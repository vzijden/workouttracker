<?xml version="1.0" encoding="utf-8"?>
<layout>

    <data>

        <variable
                name="schedulePresenter"
                type="vzijden.workout.view.home.schedule.SchedulePresenter"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            android:background="@color/background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            xmlns:app="http://schemas.android.com/apk/res-auto">

        <androidx.recyclerview.widget.RecyclerView
                android:layout_width="match_parent"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:data="@{schedulePresenter.scheduleItemViews}"
                app:changedPositions="@{schedulePresenter.changedPositions}"
                android:id="@+id/schedule_item_recycler_view">

        </androidx.recyclerview.widget.RecyclerView>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
